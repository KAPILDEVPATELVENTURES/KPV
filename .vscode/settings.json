{
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "eslint.experimental.useFlatConfig": true,
    "editor.formatOnSave": true,
    "files.exclude": {
        "**/.next": true,
        "**/node_modules": true,
        "**/out": true
    }
}